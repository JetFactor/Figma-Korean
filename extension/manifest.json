{
    "manifest_version": 3,
    "name": "Figma Korean",
    "version": "1.0",
    "description": "Figma를 한글로 사용해보세요.",
    "homepage_url": "https://github.com/v1bt/Figma-Korean/",
    "icons": {
        "64": "icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.figma.com/*"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ]
} 
